{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "es6",
      "es2017",
      "dom.iterable",
      "esnext.asynciterable"
    ],
    "allowJs": true,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": true,
    "jsx": "preserve",
    "noImplicitAny": false
  },
  "include": [
    "src"
  ],
  "exclude": [
    "src/services/typeDefs/globals/graphql-global-types.ts",
    "node_modules"
  ]
}
